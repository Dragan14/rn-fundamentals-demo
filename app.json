{
  "expo": {
    "name": "rn-fundamentals-demo",
    "slug": "rn-fundamentals-demo",
    "scheme": "rn-fundamentals-demo",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/rn-fundamentals-logo.png",
    "userInterfaceStyle": "automatic",
    "newArchEnabled": true,
    "splash": {
      "image": "./assets/rn-fundamentals-logo.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.dragan14.rnfundamentalsdemo"
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/rn-fundamentals-logo.png",
        "backgroundColor": "#ffffff"
      },
      "package": "com.dragan14.rnfundamentalsdemo"
    },
    "web": {
      "favicon": "./assets/rn-fundamentals-logo.png",
      "bundler": "metro"
    },
    "plugins": [
      "expo-router",
      [
        "expo-splash-screen",
        {
          "image": "./assets/rn-fundamentals-logo.png",
          "backgroundColor": "#fbfcff",
          "dark": {
            "image": "./assets/rn-fundamentals-logo.png",
            "backgroundColor": "#1a1c1e"
          },
          "imageWidth": 200,
          "resizeMode": "contain"
        }
      ]
    ]
  }
}
